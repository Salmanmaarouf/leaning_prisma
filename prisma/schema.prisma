// the generator reads all the prisma style code and converts it into prisma cient js fromat
generator client {
  provider =      "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  name String
}

 
